{
  "name": "Browsavulary",
  "description": "A new plugin from dand-code productions",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage", "tabs"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Pop-up Word App",
    "default_icon": "popup-icon.png"
  },
  "content_scripts": [{
    "js": ["replacement_words.js"],
    "matches": ["http://*/*",
                "https://*/*"
    ]
  }]
}