{
  "name": "PopUpWords App",
  "description": "A new plugin from dand-code productions",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage", "activeTab", "tabs"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "PopUpWords App",
    "default_icon": "PopUpWords.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "js": ["replacement_words.js"],
    "matches": ["http://*/*",
                "https://*/*"
    ]
  }]
}